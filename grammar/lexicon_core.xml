<family name="Noun" pos="N">
  <entry name="n">
    <atomcat type="n">
      <fs id="2">
        <feat attr="num">
          <featvar name="NUM"/>
        </feat>
        <feat attr="index">
          <lf><nomvar name="X" /></lf>
        </feat>
      </fs>
      <lf>
        <satop nomvar="X:sem-obj">
