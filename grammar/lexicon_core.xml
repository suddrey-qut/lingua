<family name="Adjective" pos="Adj">
  <entry name="stand-alone">
    <atomcat type="jj">
      <fs id="2">
        <feat attr="num">
          <featvar name="NUM"/>
        </feat>
        <feat attr="index">
          <lf><nomvar name="M" /></lf>
        </feat>
      </fs>
      <lf>
        <satop nomvar="M:state">
          <prop name="[*DEFAULT*]" />
        </satop>
      </lf>
    </atomcat>
  </entry>
  <entry name="Primary">
      <complexcat>
        <atomcat type="n">
          <fs inheritsFrom="2">
            <feat attr="mod-index">
              <lf>
                <nomvar name="M"/>
              </lf>
            </feat>
          </fs>
        </atomcat>
        <slash dir="/" mode="^" />
        <atomcat type="n">
          <fs id="2">
            <feat attr="index">
              <lf><nomvar name="X" /></lf>
            </feat>
          </fs>
        </atomcat>
        <lf>
          <satop nomvar="X:sem-obj">
            <diamond mode="attr">
              <nomvar name="M:state" />
              <prop name="[*DEFAULT*]" />
            </diamond>
          </satop>
        </lf>
      </complexcat>
    </entry>
</family>

